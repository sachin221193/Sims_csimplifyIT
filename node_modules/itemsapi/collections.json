[
  {
    "name": "mobiles",
    "schema": {
      "name": {"type": "string", "store": true },
      "image": {"type": "string", "display": "image" },
      "price": {"type": "float", "store": true },
      "brand": {"type": "string", "store": true },
      "os": {"type": "string", "store": true },
      "ram": {"type": "integer", "store": true },
      "rom": {"type": "integer", "store": true },
      "screen": {"type": "float", "store": true  },
      "camera": {"type": "integer", "store": true },
      "rating": {"type": "float", "store": true , "Fieldata":true}
    },
    "aggregations": {
      "barnd_name": {"type": "terms", "field": "brand", "size": 10, "title": "Brands"},
      "os_name": {"type": "terms", "field": "os", "size": 10, "title": "OS"},
      "ram_quantity": {"type": "terms", "field": "ram", "size": 10, "title": "RAM"},
      "rom_quantity": {"type": "terms", "field": "rom", "size": 10, "title": "ROM"},
      "camera_value": {"type": "terms", "field": "camera", "size": 10, "title": "Camera"},
    },
    "sortings": {
      "rating": {
        "title": "Best rating",
        "type": "normal",
        "order": "desc",
        "field": "rating"
      },
      "price": {
        "title": "Price",
        "type": "normal",
        "order": "asc",
        "field": "price"
      }
    }
  }
]